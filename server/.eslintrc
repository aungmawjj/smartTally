{ 
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "consistent-return": 2,
    "indent"           : [1, 2],
    "semi"             : [1, "always"],
    "space-unary-ops"  : 2,
    "no-console": 0
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env" : {
    "node" : true,
    "es6": true
  },
}